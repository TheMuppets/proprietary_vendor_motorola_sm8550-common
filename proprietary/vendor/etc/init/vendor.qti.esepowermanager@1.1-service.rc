on boot && property:ro.vendor.hw.nfc=ese_nq
    start qti_esepowermanager_service_1_1

service qti_esepowermanager_service_1_1 /vendor/bin/hw/vendor.qti.esepowermanager@1.1-service
    class early_hal
    user system
    group nfc system
    disabled
